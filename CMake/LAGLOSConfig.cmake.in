include(${CMAKE_CURRENT_LIST_DIR}/LAGLOSConfigVersion.cmake)

set(LAGLOS_VERSION ${Laglos_version})

set(LAGLOS_USE_HIOP @LAGLOS_USE_HIOP@)
set(LAGLOS_USE_BOOST @LAGLOS_USE_BOOST@)

set(LAGLOS_CXX_COMPILER "@CMAKE_CXX_COMPILER@")
set(LAGLOS_CXX_FLAGS "@CMAKE_CXX_FLAGS@")

@PACKAGE_INIT@

set(LAGLOS_INCLUDE_DIRS "@PACKAGE_INCLUDE_INSTALL_DIRS@")
foreach (dir ${LAGLOS_INCLUDE_DIRS})
  set_and_check(LAGLOS_INCLUDE_DIR "${dir}")
endforeach (dir "${LAGLOS_INCLUDE_DIRS}")

set_and_check(LAGLOS_LIBRARY_DIR "@PACKAGE_LIB_INSTALL_DIR@")

if (NOT TARGET Laglos)
  include(${CMAKE_CURRENT_LIST_DIR}/LaglosTargets.cmake)
endif (NOT TARGET Laglos)

check_required_components(Laglos)

set(LAGLOS_LIBRARIES Laglos)
